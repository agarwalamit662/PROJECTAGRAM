{
  "entrypoint": "index.html",
  "shell": "src/shop-app.html",
  "fragments": [
    "src/shop-teams.html",
    "src/shop-list.html",
    "src/shop-rewards.html",
    "src/shop-viewevent.html",
    "src/shop-rewardslist.html",
    "src/shop-detail.html",
    "src/shop-adduser.html",
    "src/shop-addevent.html",
    "src/shop-addrandr.html",
    "src/lazy-resources.html",
    "src/shop-404-warning.html"
  ],
  "sources": [
   "src/**/*",
   "data/**/*",
   "images/**/*",
   "bower.json",
   "bower_components/app-storage/app-indexeddb-mirror/*.js"
  ],
  "extraDependencies": [
	"app.yaml",    
	"manifest.json",
  "bower_components/webcomponentsjs/webcomponents-lite.min.js"
  ],
  "lint": {
    "rules": ["polymer-2-hybrid"]
  },
  "builds": [
    {
      "preset": "es5-bundled"
    },
    {
      "preset": "es6-unbundled"
    }
  ]
}
